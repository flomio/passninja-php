{
    "name": "passninja/passninja",
    "description": "A PassNinja API helper library for PHP",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Passninja\\Passninja\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "8.2.0"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3"
    },
    "require": {
        "guzzlehttp/guzzle": "8.0.x-dev"
    },
    "minimum-stability": "dev"
}
